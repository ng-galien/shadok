{
  "testEnvironment": "node",
  "testMatch": [
    "**/tests/**/*.test.js",
    "**/__tests__/**/*.js",
    "**/*.test.js"
  ],
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/**/*.test.js"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ]
}
